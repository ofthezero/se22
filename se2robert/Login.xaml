<Window x:Class="Uwu2.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uwu2"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <Image Margin="93,10,93,241" Source="/DS2017_TP09_black@2x.png" Stretch="Fill"/>
        <TextBox x:Name="Login" HorizontalAlignment="Center" Margin="0,287,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="308"/>
        <Button x:Name="Log" Content="Login" HorizontalAlignment="Left" Margin="288,374,0,0" VerticalAlignment="Top" Width="59" Click="Log_Click"/>
        <Button x:Name="Exit" Content="Exit" HorizontalAlignment="Left" Margin="432,374,0,0" VerticalAlignment="Top" Width="59"/>
        <TextBlock HorizontalAlignment="Left" Margin="185,287,0,0" TextWrapping="Wrap" Text="Username:" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="185,325,0,0" TextWrapping="Wrap" Text="Password:" VerticalAlignment="Top"/>
        <PasswordBox x:Name="Pass" HorizontalAlignment="Center" Margin="0,324,0,0" VerticalAlignment="Top" Width="308"/>
        <TextBlock x:Name="error" HorizontalAlignment="Left" Margin="93,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Center"/>
    </Grid>
</Window>
